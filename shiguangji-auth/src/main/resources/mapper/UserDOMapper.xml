<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace等于mapper接口类的全限定名,这样实现对应 -->
<mapper namespace="com.xiaowei.shiguangji.auth.domain.mapper.UserDOMapper">

    <select id="selectByPhone" resultType="com.xiaowei.shiguangji.auth.domain.dataobject.UserDO">
        select id, password from t_user where phone = #{phone}
    </select>
</mapper>